@{
    ViewBag.Title = "GetTeachers";
    Layout = "~/Views/Shared/UALogin.cshtml";
}
@model IEnumerable<MySliderProject.Models.Teacher>

    @section title
    {
        <title>Easy Course Registration System | Courses</title>
    }
 
<div class="main_btm1"><!-- start main_btm -->
    <div class="container">
	    <div class="technology">
		    <div class="technology_main">
                @foreach(var teacher in Model){
			    <div class="technology_list col-md-6">
                    <h2>@teacher.Name</h2>
                     <h4>Teacher ID : @teacher.TId</h4>
                     <h4>Course ID : @teacher.CId</h4>
				    <h4>Designation : @teacher.Designation</h4>
                    <h4>Qualification : @teacher.Qualification</h4>
                    <h4>Phone : @teacher.Phone</h4>
                    				    
				    <div class="clearfix"></div>

          		    <div class="read_btn">
                        <a href="~/Home/NowEnroll/@teacher.TId" class="btn">Enroll Me</a>
				    </div>	

			    </div>
                }
			  
	    </div>
    </div>
</div>
</div>
